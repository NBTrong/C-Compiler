PROGRAM Example8;

VAR a: ARRAY[10] OF INTEGER;
    MAX: INTEGER;

BEGIN
  MAX := 0;
  FOR i := 1 TO 10 DO
    BEGIN
      A[i] := READI;
    END;
  FOR i := 1 TO 10 DO
    BEGIN
      IF A[i] >= MAX THEN
        MAX := A[i];
    END;
  FOR i := 1 TO 10  DO
    BEGIN
      IF A[i] = MAX THEN
        CALL WRITEI(A[i]);
    END;
END.